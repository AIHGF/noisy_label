net: "models/clothing1M/ntype_trainval.prototxt"

test_iter: 75
test_interval: 200
test_initialization: true

base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 3000
max_iter: 5000

display: 20
average_loss: 20

momentum: 0.9
weight_decay: 0.0005

snapshot: 5000
snapshot_prefix: "external/exp/snapshots/clothing1M/ntype"
solver_mode: GPU
