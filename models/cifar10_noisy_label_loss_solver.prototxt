net: "models/cifar10_noisy_label_loss_trainval.prototxt"

test_iter: 100
test_interval: 500
test_initialization: true

base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 4000
max_iter: 6000

display: 100
average_loss: 100

momentum: 0.9
weight_decay: 0.004

snapshot: 8000
snapshot_prefix: "external/exp/models/cifar10_noisy_label_loss"
solver_mode: CPU
